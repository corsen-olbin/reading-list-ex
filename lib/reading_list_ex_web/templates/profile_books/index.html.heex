<h1>Listing Books</h1>

<table>
  <thead>
    <tr>
      <th>Isbn 13</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for book <- @books do %>
    <tr>
      <td><%= book.book.isbn_13 %></td>

      <td>
        <span><%= link "Show", to: Routes.book_path(@conn, :show, book.book) %></span>
      </td>
    </tr>
<% end %>
  </tbody>
</table>
